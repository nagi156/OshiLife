<div class="container">
  <div class="row">
    <div class="col">
      <%= form_with model: @post do |f| %>
        <div class="form-group">
          <!--完成画像-->
          <%= f.file_field :post_image, accept: "image/*" %>
        </div>
        <div class="form-group">
          <%= f.label "タイトル " %>
          <%= f.text_field :title, class: "form-control" %>
        </div>
        
        <div class="material">
        </div>
      
      
      
      
      <% end %>
    </div>
  </div>
</div>